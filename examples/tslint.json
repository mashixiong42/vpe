{
  "extends": "../tslint.json",
  "linterOptions": {
    "exclude": [
      "config/**/*.js",
      "node_modules/**/*.ts",
      "coverage/lcov-report/*.js"
    ]
  },
  "rules": {
    "ordered-imports": false,
    "no-unused-variable": false,
    "no-unused-expression": false,
    "no-console": false
  }
}
